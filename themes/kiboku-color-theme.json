  "semanticTokenColors": {
    "variable": "#f2f4f8",
    "parameter": "#f2f4f8",
    "parameter.label": "#8193B1",
    "property": "#f2f4f8",
    "class": "#bea1f7",
    "namespace": "#ff9ce6",
    "variable.defaultLibrary": "#bea1f7",
    "method.defaultLibrary": "#69a4fc",
    "method": "#69a4fc",
    "method.declaration": "#69a4fc",
    "function": "#69a4fc",
    "type": "#ff9ce6"
